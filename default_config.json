            "when": ["สวัสดี"],
            "do": "say",
            "action": "hello"
        },
        {
            "key": "2",
            "name": "reload",
            "when": ["reload"],
            "do": "press",
            "action": "r"
        },
        {
            "key": "3",
            "name": "map",
            "when": ["map"],
